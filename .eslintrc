{
  "parser": "@typescript-eslint/parser",
  "plugins": ["import"],
  "rules": {
    "import/no-unresolved": "off",
    "import/extensions": "off",
    "react/forbid-prop-types": 0,
    "class-methods-use-this": 0,
    "no-confusing-arrow": 0,
    "quote-props": ["error", "consistent-as-needed"]
  }
}
